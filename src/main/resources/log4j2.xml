<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="ayesh-debug" packages=""  monitorInterval="30">
    <Appenders>
        <Console name="my_log" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %C %p %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="trace">
            <AppenderRef ref="my_log"/>
        </Root>
    </Loggers>
</Configuration>
